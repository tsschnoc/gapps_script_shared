</head>  


<body>
  <div class="ui-widget">
    <div id='ny' tip='hi tip'>no Calls</div>
  	<input id="searchfield" class="ui-autocomplete-input" autocomplete="off" role="textbox" aria-autocomplete="list" aria-haspopup="true" style="width: 110px;">
    <div id='itemDetail'></div>
  </div>  
  
  
  
  <script type="text/template" id="ResultItemListView-template">
    <% if (type == "SF") { %>  
      <img border="0" src="http://www.salesforce.com/favicon.ico" alt="google contact" width="10" height="10">
    <% } %>
    <% if (type == "Google") { %>  
      <img border="0" src="http://www.ltech.com/img/icon_contact.jpg" alt="google contact" width="10" height="10">
    <% } %>
    <%= label %>
    <div style="font-size: 70%; ">
      <%= label %>
    </div>
  </script>
  
  <script type="text/template" id="ResultItemDetailView-template">
    <% if (type == "SF") { %>  
      <img border="0" src="http://www.salesforce.com/favicon.ico" alt="google contact" width="10" height="10">
    <% } %>
    <% if (type == "Google") { %>  
      <img border="0" src="http://www.ltech.com/img/icon_contact.jpg" alt="google contact" width="10" height="10">
    <% } %>
    Detail
    <%= label %>
    <div style="font-size: 70%; ">
      <%= label %>
    </div>
  </script>

  
  <script type="text/template" id="nix-template">        
    
    <% if (done) { %>
      <a id="clear-completed">Clear <%= done %> completed <%= done == 1 ? 'item' : 'items' %></a>
    <% } %>
    
    <div class="todo-count"><b><%= remaining %></b> <%= remaining == 1 ? 'item' : 'items' %> left</div>
  </script>
  
</body>


